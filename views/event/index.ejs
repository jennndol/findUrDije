<h2>Genre List</h2>
<table>
  <tr>
    <td>No</td>
    <td>Name</td>
    <td>Detail</td>
    <td>Date</td>
    <td>Created By</td>
    <td>Action</td>
  </tr>
  <% events.forEach((event, i) => { %>
    <tr>
      <td>
        <%= i + 1 %>
      </td>
      <td>
        <%= event.name %>
      </td>
      <td>
        <%= event.detail %>
      </td>
      <td>
        <%= event.date %>
      </td>
      <td>
        <%= event.DJSeeker.name %>
      </td>
      <td>
        <a href="/events/edit/<%= event.id %>">Edit</a>
        <a href="/events/delete/<%= event.id %>">Delete</a>
      </td>
    </tr>
    <% }); %>
</table>
